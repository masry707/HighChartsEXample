@import "variables";
// @import "_variables_json.json";

.theme1 {
  /* CSS Document */

  /* FLAG=global
  ----------------------------------------------- */
  body{color: #666666; background: $P200;}
  a{color: $P50;}
  a:hover{color:#06F;}
  em{color:#06F;}
  strong{color:#666;}

  /* FLAG=containers
  ----------------------------------------------- */
  header,
  main,
  footer,
  section {
    background-color: #ccc;
    border: 1px solid #fff;
  }
}

